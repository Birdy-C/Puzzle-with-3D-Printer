// Function to print the state name
void printState(State s) {
  switch (s) {
    case SHOW_EYES:
      Serial.println("SHOW_EYES");
      break;
    case SHOW_DATE:
      Serial.println("SHOW_DATE");
      break;
    case VIBRATION_DETECTED:
      Serial.println("VIBRATION_DETECTED");
      break;
    default:
      Serial.println("Unknown State");
      break;
  }
  return;
}

State previousState = SHOW_DATE;
State currentState = SHOW_DATE;  // Initial state is showing eyes

unsigned long vibrationDetectedMillis = 0;      // Stores the last time the screen was updated
unsigned long stateChangeMillis = 0;            // Stores the last time the state was updated
unsigned long stateChangeIntervalTimer = 5000;  // Interval for showing eyes and date (5000 ms = 5 seconds)
unsigned long stateChangeIntervalEye = 10000;   // Interval for showing eyes and date (5000 ms = 5 seconds)
unsigned long stateChangeIntervalVib = 5000;    // Interval for showing eyes and date (5000 ms = 5 seconds)

unsigned long animation_started = 0;
int animation_loop = 0;

void loop() {
  RtcDateTime now = Rtc.GetDateTime();
  int vibrationValue = analogRead(VIBRATION_SENSOR);

  // Get the current time
  unsigned long currentMillis = millis();

  previousState = currentState;
  // Check vibration threshold (non-blocking)
  if (vibrationValue > THRESHOLD) {
    Serial.println("vibration detected");
    currentState = VIBRATION_DETECTED;
    vibrationDetectedMillis = currentMillis;

  } else {
    // Update the state based on time intervals
    if (previousState == SHOW_EYES && currentMillis - stateChangeMillis >= stateChangeIntervalEye) {
      currentState = SHOW_DATE;  // Switch to SHOW_DATE after showing eyes
    } else if (previousState == SHOW_DATE && currentMillis - stateChangeMillis >= stateChangeIntervalTimer) {
      currentState = SHOW_EYES;  // Switch back to SHOW_EYES after showing the date
    } else if (previousState == VIBRATION_DETECTED && currentMillis - vibrationDetectedMillis >= stateChangeIntervalVib) {
      currentState = SHOW_EYES;
    }
  }

  if (currentState != previousState) {
    stateChangeMillis = currentMillis;
    animation_started = currentMillis;
    animation_loop = 0;
    clearScreen();
    // Handle different states
    switch (currentState) {
      case VIBRATION_DETECTED:
        Serial.println("Drawing Hearts.");
        animation_loop = drawHearts(animation_loop);
        break;

      case SHOW_EYES:
        Serial.println("Showing Eyes.");
        animation_loop = drawEyes(animation_loop);
        break;

      case SHOW_DATE:
        Serial.println("Showing Date.");
        printDate(now);
        break;

      default:
        break;
    }
  }
  if (currentMillis - animation_started > animation_interval) {
    animation_started = currentMillis;
    animation_loop = animation_loop + 1;
    switch (currentState) {
      case VIBRATION_DETECTED:
        animation_loop = drawHearts(animation_loop);
        break;

      case SHOW_EYES:
        animation_loop = drawEyes(animation_loop);
        break;

      default:
        break;
    }
  }
  // unsigned long endMillis = millis();
  // if (endMillis - currentMillis < 1000) {
  //   delay(1000 - (endMillis - currentMillis));
  // }
}