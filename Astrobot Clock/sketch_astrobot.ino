#include <UTFT.h>
#include <ThreeWire.h>
#include <RtcDS1302.h>

// Define states for the state machine
enum State {
  SHOW_EYES,
  SHOW_DATE,
  VIBRATION_DETECTED
};

#define VIBRATION_SENSOR A7  // Define Vibration Sensor pin
#define THRESHOLD 500        // Define a threshold for vibration detection

// DS1302 CLK/SCLK --> 5
// DS1302 DAT/IO --> 4
// DS1302 RST/CE --> 2
ThreeWire myWire(4, 5, 2);  // IO, SCLK, CE
RtcDS1302<ThreeWire> Rtc(myWire);

// TFT-LCD PIN Arduino Pin 说明
// GND GND 电源地
// VCC 3.3/5V 电源正3.3V 或5V 都可
// SCL A1 时钟线
// SDA A2 数据线
// RES A4 复位线
// DC A3 数据/命令选择线
// CS A5 片选
// BL A0 背光开关
// Declare an instance of the class
UTFT myGLCD(QD_TFT180C, A2, A1, A5, A4, A3);  // Remember to change the model parameter to suit your display module!
// Declare which fonts we will be using
extern uint8_t SevenSegNumFont[];

#define countof(a) (sizeof(a) / sizeof(a[0]))

// 格式化打印日期时间
void printDateTime(const RtcDateTime& dt) {
  char datestring[20];

  snprintf_P(datestring,
             countof(datestring),
             PSTR("%02u/%02u/%04u %02u:%02u:%02u"),
             dt.Month(),
             dt.Day(),
             dt.Year(),
             dt.Hour(),
             dt.Minute(),
             dt.Second());
  Serial.print(datestring);
}

void setupRtc() {

  // 打印编译时间
  Serial.print("compiled: ");
  Serial.print(__DATE__);
  Serial.println(__TIME__);

  // 初始化RTC模块
  Rtc.Begin();

  // 获取编译时间并打印
  RtcDateTime compiled = RtcDateTime(__DATE__, __TIME__);
  printDateTime(compiled);
  Serial.println();

  // 检查RTC模块是否有效，如果无效则设置为编译时间
  if (!Rtc.IsDateTimeValid()) {
    Serial.println("RTC lost confidence in the DateTime!");
    Rtc.SetDateTime(compiled);
  }
  // 检查RTC模块是否写保护，如果是则取消写保护
  if (Rtc.GetIsWriteProtected()) {
    Serial.println("RTC was write protected, enabling writing now");
    Rtc.SetIsWriteProtected(false);
  }
  // 检查RTC模块是否在运行，如果没有则启动
  if (!Rtc.GetIsRunning()) {
    Serial.println("RTC was not actively running, starting now");
    Rtc.SetIsRunning(true);
  }
  // 获取当前时间并与编译时间进行比较，如果RTC时间早于编译时间，则设置为编译时间
  RtcDateTime now = Rtc.GetDateTime();
  if (now < compiled) {
    Rtc.SetDateTime(compiled);
  }
}

void setupLCD() {
  // Setup the LCD
  myGLCD.InitLCD();
  myGLCD.setFont(SevenSegNumFont);
  myGLCD.clrScr();
  myGLCD.setContrast(64);
  myGLCD.fillScr(0, 0, 0);  // Fills screen with black
}


void setup() {
  // 初始化串口通信
  Serial.begin(9600);

  randomSeed(analogRead(0));

  setupRtc();
  setupLCD();
}

